   "source": [
    "def sentiment(df):\n",
    "    df['sentiment']='-'\n",
    "    df.loc[df['score']<=2,'sentiment']='negative'\n",
    "    df.loc[df['score']==3,'sentiment']='neutral'\n",
    "    df.loc[df['score']>=4,'sentiment']='positive'\n",
    "    \n",
    "    df = pd.get_dummies(df,columns=['sentiment']).reset_index(drop=True).drop(columns=['score'])\n",
    "    return df"
   ]
  },
